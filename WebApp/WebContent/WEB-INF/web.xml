<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>EnzymeEfficiency</display-name>
	
	<servlet>
		<description>
		Servlet to execute AJAX driven model development</description>
		<display-name>AjaxBuildModel</display-name>
		<servlet-name>AjaxBuildModel</servlet-name>
		<servlet-class>
		org.enzymes.AjaxBuildModel</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AjaxBuildModel</servlet-name>
		<url-pattern>/AjaxBuildModel</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<description>
		Servlet to execute AJAX driven model testing</description>
		<display-name>AjaxTestModel</display-name>
		<servlet-name>AjaxTestModel</servlet-name>
		<servlet-class>
		org.enzymes.AjaxTestModel</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AjaxTestModel</servlet-name>
		<url-pattern>/AjaxTestModel</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
</web-app>
